{
    "manifest_version": 2,
    "name": "Spade",
    "version": "0.1",
    "background":{
        "scripts":["background.js"],
        "persistent": false
    },
    "content_scripts":[{
        "matches":[
            "<all_urls>"
        ],
        "js": ["./p5/p5.js", "main.js"]
    }],
    "permissions":[
        "storage",
        "tabs"
    ],
    "browser_action":{
        "default_popup": "popup.html"
    }
}